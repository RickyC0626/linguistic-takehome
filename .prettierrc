{
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": false,
  "bracketSameLine": true,
  "semi": true,
  "trailingComma": "none",
  "endOfLine": "lf",
  "printWidth": 80,
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
